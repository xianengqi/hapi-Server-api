{
  "name": "hapi-server-api",
  "version": "1.0.0",
  "description": "一款脚手架工具，方便自己日后开发",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "supervisor app.js",
    "init": "node_modules/.bin/sequelize init",
    "createdb:dev": "node_modules/.bin/sequelize db:create",
    "createdb:prod": "node_modules/.bin/sequelize db:create --env production",
    "createtable": "node_modules/.bin/sequelize migration:create --name",
    "createtableall": "node_modules/.bin/sequelize db:migrate",
    "createtableundo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "createtableundo:to": "node_modules/.bin/sequelize db:migrate:undo:all --to",
    "initdatadb": "node_modules/.bin/sequelize seed:create --name",
    "initdatadball": "node_modules/.bin/sequelize db:seed:all",
    "initdatadbseed": "node_modules/.bin/sequelize db:seed --seed",
    "initdataundo": "node_modules/.bin/sequelize db:seed:undo --seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xianengqi/hapi-Server-api.git"
  },
  "author": "XiaNengQi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xianengqi/hapi-Server-api/issues"
  },
  "homepage": "https://github.com/xianengqi/hapi-Server-api#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "env2": "^2.2.2",
    "good": "^7.3.0",
    "good-console": "^7.1.0",
    "good-file": "^6.0.1",
    "good-http": "^6.1.3",
    "good-squeeze": "^5.1.0",
    "hapi": "^16.7.0",
    "hapi-auth-jwt2": "^7.4.1",
    "hapi-pagination": "^1.22.0",
    "hapi-swagger": "^7.10.0",
    "inert": "^4.2.1",
    "insert": "^1.0.1",
    "joi": "^13.7.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "package": "^1.0.1",
    "vision": "^4.1.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  }
}
